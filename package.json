{
  "name": "snapchain-shuttle",
  "version": "1.0.0",
  "description": "Custom Farcaster Snapchain data sync to PostgreSQL",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "start": "tsx src/app.ts start",
    "start:prod": "node dist/app.js start",
    "build": "tsc",
    "dev": "tsx watch src/app.ts start",
    "stats": "tsx src/app.ts stats",
    "backfill": "tsx src/app.ts backfill",
    "worker": "tsx src/app.ts worker",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@farcaster/shuttle": "^0.9.2",
    "@farcaster/hub-nodejs": "^0.15.2",
    "@farcaster/hot-shots": "^10.0.0",
    "commander": "11.0.0",
    "dotenv": "16.4.5",
    "ioredis": "^5.3.2",
    "kysely": "^0.26.1",
    "neverthrow": "^6.0.0",
    "pg": "^8.12.0",
    "pg-cursor": "^2.10.3",
    "pino": "^8.12.1",
    "pino-pretty": "^10.0.0",
    "sql-formatter": "^13.0.0",
    "tiny-typed-emitter": "^2.1.0",
    "bullmq": "^5.7.1"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "11.0.0",
    "@types/pg": "^8.10.3",
    "@types/pg-cursor": "^2.7.0",
    "@types/node": "^20.11.30",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0"
  },
  "keywords": ["farcaster", "snapchain", "postgres", "shuttle"],
  "author": "ohsnap",
  "license": "MIT"
}